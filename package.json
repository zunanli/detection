{
  "name": "webview-monitor",
  "version": "1.0.0",
  "description": "WebView white screen monitoring tool with offline package support",
  "main": "server/index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "cd client && npm test",
    "eject": "cd client && npm run eject",
    "clean": "rm -rf node_modules client/node_modules client/build"
  },
  "keywords": [
    "webview",
    "monitoring",
    "white-screen",
    "offline-package",
    "puppeteer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^21.7.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
